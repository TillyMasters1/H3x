					NotifiyFrame.Size = UDim2.new(0.182281062, 0, 0.941189876, 0)
					close.Position = UDim2.new(0.164460286, 0, 0.122446127, 0)
					Description.Position = UDim2.new(0.0066191447, 0, 0.603484452, 0)
					Description.Size = UDim2.new(0.17260693, 0, 0.314861476, 0)
					Title.Position = UDim2.new(0.0066191447, 0, 0.349846065, 0)
					Title.Size = UDim2.new(0.17260693, 0, 0.253638417, 0)
					Name.Position = UDim2.new(0.0229124241, 0, 0.0874615163, 0)
					Name.Size = UDim2.new(0.134419546, 0, 0.253638417, 0)
					Notification1:TweenSize(UDim2.new(1, 0, 0.096, 0))
				end
				
				function txt:Del()
					Notification1:TweenSize(UDim2.new(0, 0, 0, 0))
					wait(1.1)
					Notification1:Destroy()
				end

				wait(duration)

				Notification1:TweenSize(UDim2.new(0, 0, 0, 0))

				wait(1.1)

				Notification1:Destroy()
			end
		end

	else
		insertContainer(game.Players.LocalPlayer)
		wait(0.05)
		module.push(table)
	end
	
	return txt;

end

return module
